<?xml version="1.0" encoding="UTF-8"?>
<localEntry key="SFTP_LOCATION" xmlns="http://ws.apache.org/ns/synapse">
	<file.init>
        <connectionType>SFTP</connectionType>
        <password>{get-property('env', 'sftp_password')}</password>
        <host>{get-property('env', 'sftp_hostname')}</host>
        <fileLockScheme>Local</fileLockScheme>
        <port>22</port>
        <name>SFTP_LOCATION</name>
        <username>{get-property('env', 'sftp_username')}</username>
    </file.init>
</localEntry>
